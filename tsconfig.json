{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": ["DOM"],
    "module": "ES2022",
    "target": "ESNext",
    "moduleResolution": "node",
    //"noImplicitAny": true,
    "removeComments": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
